{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
	"quotes": ["warn", "double"],
	"max-len": ["warn"],
	"comma-dangle": ["error", "never"],
	"require-jsdoc": ["warn"]
    }
}
